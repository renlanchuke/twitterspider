                url: 'http://www.appledaily.com.tw/index/getpv/?callback=',
                dataType: 'jsonp',
                success: function(data){
                    $('#pv').html(data.appledaily_view);
                }
        });

        $(window).scroll(function() {
          if($(this).scrollTop()>130&&($(this).scrollTop()-(-800)<$(document).height()-350)){
              $('#door').css('position','fixed').css('top','-45px');
          }else{
              $('#door').css('position','relative').css('top','0px');
          }
        });

        $.tools.dateinput.localize("tw", {
            months:       '一月,二月,三月,四月,五月,六月,七月,八月,九月,十月,十一月,十二月',
            shortMonths:  '一,二,三,四,五,六,七,八,九,十,十一,十二',
            days:         '日,一,二,三,四,五,六',
            shortDays:    '日,一,二,三,四,五,六'
        });

        $("#DailyDate").dateinput({lang: 'tw',selectors: true,format: 'yyyymmdd',trigger: true ,min:'2003-05-02',max:'2016-09-24',offset: [0,-50],speed: 'fast',yearRange:[-20,1]});
        $("#DailyDate").change(function() {location.href="http://www.appledaily.com.tw/appledaily/archive/"+$("#DailyDate").val();});
        $(".nbtn a.caltrigger").append("昔日新聞");
        $(".mmvnv a.caltrigger").append("昔日動新聞");
    });
  </script>

