  function validate_form(obj){
    if($('#searchFieldAnd').val() == ''){
      alert("搜尋字串不能為空值");
      return false;
    }
    if($('#start_date').val().length != 10 || $('#end_date').val().length != 10){
      alert("區間格式不正確 , 請輸入 YYYY-MM-DD !");
      return false;
    }

    if($('#start_date').val() >= $('#end_date').val()){
      alert("區間開始日期不得大於結束日期 !");
      return false;
    }
    return true;
  }
